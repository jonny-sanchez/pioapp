{
  "expo": {
    "name": "pioapp",
    "slug": "pioapp",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/images/LOGOPINULITO.png",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      "expo-notifications",
      "@react-native-firebase/app"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/LOGOPINULITO.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/LOGOPINULITO.png",
      "resizeMode": "contain",
      "backgroundColor": "#fcb900"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "config": {
        "googleMapsApiKey": "process.env.GOOGLE_MAPS_API_KEY"
      },
      "supportsTablet": true,
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "process.env.GOOGLE_MAPS_API_KEY"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LOGOPINULITO.png",
        "backgroundColor": "#fcb900"
      },
      "package": "com.anonymous.pioapp",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "d10fa382-0583-4d8e-a439-c1e3fb01ab7d"
      }
    },
    "owner": "digu123"
  }
}
