{
  "expo": {
    "name": "pioapp",
    "slug": "pioapp",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/images/LOGOPINULITO.png",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-font"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/LOGOPINULITO.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/LOGOPINULITO.png",
      "resizeMode": "contain",
      "backgroundColor": "#fcb900"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LOGOPINULITO.png",
        "backgroundColor": "#fcb900"
      },
      "package": "com.anonymous.pioapp"
    }
  }
}
